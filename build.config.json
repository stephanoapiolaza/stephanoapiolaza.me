{
  "paths": {
    "src": "./app",
    "media": "./app/media",
    "build": "./build",
    "buildCss": "./build/css",
    "buildJs": "./build/js",
    "buildMedia": "./build/media",
    "temp": "./.tmp",
    "scss": "./app/scss",
    "js": "./app/js"
  },
  "package": {
    "path": "./build",
    "name": "production.zip",
    "css": {
      "name": "master.css",
      "include": [
        "./build/css/master.css",
        "./build/assets/material-design-icons/materialdesignicons.min.css",
        "./build/assets/jquery.mCustomScrollbar/jquery.mCustomScrollbar.css",
        "./build/assets/bootstrap/bootstrap.min.css",
        "./build/css/typography.css",
        "./build/css/theme.css",
        "./build/css/responsive.css",
        "./build/css/color.css",
        "./build/css/custom.css"
      ]
    },
    "js": {
      "name": "master.js",
      "include": [
        "./build/js/master.js",
        "./build/assets/jquery/jquery-2.2.3.min.js",
        "./build/assets/jquery.mCustomScrollbar/jquery.mCustomScrollbar.concat.min.js",
        "./build/assets/bootstrap/bootstrap.min.js",
        "./build/assets/jquery-placeholder/jquery.placeholder.min.js",
        "./build/assets/isotope/imagesloaded.pkgd.min.js",
        "./build/assets/isotope/isotope.pkgd.min.js",
        "./build/js/gradient/gradient.js",
        "./build/assets/social-likes/social-likes.min.js",
        "./build/assets/smoothscroll-for-websites/SmoothScroll.js",
        "./build/assets/contact/jqBootstrapValidation.js",
        "./build/js/theme.js",
        "./build/js/contact/contact-me.js"
